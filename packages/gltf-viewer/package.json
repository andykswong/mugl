{
  "name": "gltf-viewer",
  "version": "1.0.0",
  "description": "glTF 2.0 model viewer powered by mugl",
  "private": "true",
  "dependencies": {
    "@muds/gltf": "^0.1.0",
    "mugl": "^0.2",
    "munum": "^0.1"
  },
  "scripts": {
    "copy-gl-const": "copyfiles -u 4 ../../js/gpu/gl-const.ts ./src/",
    "prebuild": "npm run copy-gl-const",
    "build": "npm run webpack",
    "webpack": "webpack",
    "build:shaders": "node scripts/shader.mjs",
    "server": "cross-env NODE_ENV=development webpack serve",
    "start": "npm run server"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20",
    "@babel/preset-typescript": "^7.18",
    "babel-loader": "^9.1",
    "copy-webpack-plugin": "^11.0",
    "copyfiles": "^2.4",
    "cross-env": "^7.0",
    "glslify": "^7.1",
    "glslify-import": "^3.1",
    "rimraf": "^3.0",
    "source-map-loader": "^4.0",
    "webpack": "^5.74",
    "webpack-cli": "^4.10",
    "webpack-dev-server": "^4.9",
    "webpack-glsl-minify": "^1.5"
  },
  "glslify": {
    "transform": [
      "glslify-import"
    ]
  }
}
