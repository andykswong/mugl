{
  "entries": [
    "./assembly/index.ts"
  ],
  "options": {
    "sourceMap": true,
    "exportRuntime": true,
    "runtime": "incremental",
    "disableWarning": [ 235 ]
  },
  "targets": {
    "debug": {
      "outFile": "dist/mugl.wasm",
      "textFile": "dist/mugl.wat",
      "debug": true
    },
    "release": {
      "outFile": "dist/mugl.wasm",
      "optimizeLevel": 3,
      "shrinkLevel": 2,
      "converge": false,
      "noAssert": true
    }
  }
}
